#include<stdio.h>

int main()
{
    int n;

    float temp=0, sqrt;

    printf("Enter n: \n");

    scanf("%d", &n);

    sqrt = n / 2;
   
    while(sqrt != temp){
       
        temp = sqrt;

        sqrt = ( n/temp + temp) / 2;
    }

    printf("The square root of '%d' is '%.3f'", n, sqrt);
    return 0;
}
